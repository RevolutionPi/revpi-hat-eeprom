{
    "version": 1,
    "vstr": "KUNBUS GmbH",
    "pstr": "RevPi ExampleDevice 8GB",
    "pid": 666,
    "prev": 3,
    "pver": 333,
    "dtstr": "revpi-example-2022",
    "gpiobanks": [
        {
            "drive": "8mA",
            "slew": "default",
            "hysteresis": "enable",
            "gpios": [
                {
                    "gpio": 2,
                    "fsel": "input",
                    "pull": "default"
                },
                {
                    "gpio": 3,
                    "fsel": "output",
                    "pull": "none"
                },
                {
                    "gpio": 4,
                    "fsel": "alt1",
                    "pull": "up",
                    "comment": "This configures the I2C1 SCL"
                }
            ]
        }
    ]
}
